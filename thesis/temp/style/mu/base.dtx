% Define the running heading style.
\def\ps@thesisheadings{%
  \def\chaptermark##1{%
    \markright{%
      \ifnum\c@secnumdepth >\m@ne
        \thechapter.\ %
      \fi ##1}}
  \let\@oddfoot\@empty
  \let\@oddhead\@empty
  \def\@oddhead{%
    \vbox{%
      \hbox to \textwidth{%
      \hfil{\sc\rightmark}}%
      \vskip 4pt\hrule}}
  \if@twoside
